{
  "name": "vue-osm-autocomplete",
  "version": "0.0.1",
  "description": "A Vue 3 autocomplete component for location search using OpenStreetMap's Nominatim API.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/Dantescur/vue-osm-autocomplete#readme",
  "bugs": {
    "url": "https://github.com/Dantescur/vue-osm-autocomplete/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dantescur/vue-osm-autocomplete.git"
  },
  "author": "Daniel <daniel@thedaniweb.eu.org>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.17",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/test-utils": "^2.4.6",
    "bumpp": "^10.1.0",
    "happy-dom": "^17.4.7",
    "tsdown": "^0.11.9",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.1.3",
    "vue": "^3.5.13",
    "vue-tsc": "^3.0.3"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "dependencies": {
    "@vueuse/core": "^13.6.0"
  }
}
